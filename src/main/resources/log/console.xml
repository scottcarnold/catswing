<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO" packages="org.xandercat.swing.log">
	<Appenders>
		<Console name="console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d %-5p %c{1} - %m%n"/>
		</Console>
	</Appenders>
	<Loggers>
		<Root level="[level]">
			<AppenderRef ref="console" />
		</Root>
	</Loggers>
</Configuration>